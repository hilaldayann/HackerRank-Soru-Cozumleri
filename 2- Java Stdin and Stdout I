#https://www.hackerrank.com/challenges/java-stdin-and-stdout-1/problem

Scanner scan = new Scanner(System.in);
        int a;
        
        a = scan.nextInt();
        System.out.println(a);
        
        a = scan.nextInt();
        System.out.println(a);
        
        a = scan.nextInt();
        System.out.println(a);
